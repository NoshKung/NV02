#include <stdio.h>

int main(void) {
  int score;
  printf("Input score => ");
  scanf("%d",&score);
if (score >= 80 && score <= 100)
  printf("Grade = A");
else if (score >= 75 && score <= 79)
  printf("Grade = B+");
else if (score >= 70 && score <= 74)
  printf("Grade = B");
else if (score >= 65 && score <= 69)
  printf("Grade = c+");
else if (score >= 60 && score <= 64)
  printf("Grade = c");
else if (score >= 55 && score <= 59)
  printf("Grade = D+");
else if (score >= 51 && score <= 54)
  printf("Grade = D");
else if (score >= 0 && score <= 48)
  printf("Grade = F");
}
